        mc:Ignorable="d"
        Title="DoVolunteerJobWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="TeamIds" HorizontalAlignment="Left" Margin="234,66,0,0" VerticalAlignment="Top" Width="120"/>
