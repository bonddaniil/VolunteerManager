        <Button x:Name="BigRedButton" Content="Волонтеримо!" HorizontalAlignment="Center" Margin="0,112,0,0" VerticalAlignment="Top" Width="108" Height="41" Click="BigRedButton_Click"/>
        <DataGrid x:Name="ObjectDG" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,174,0,92"/>
        <Button x:Name="ToMenu" Content="У меню" HorizontalAlignment="Left" Margin="660,367,0,0" VerticalAlignment="Top" Height="41" Width="108" Click="ToMenu_Click"/>
        <DataGrid x:Name="TeamDG" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,586,271"/>

    </Grid>
</Window>
